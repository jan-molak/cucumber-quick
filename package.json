{
	"name": "cucumberquick",
	"displayName": "Cucumber Quick",
	"description": "Cucumber Quick - a quick cucumber executor",
	"version": "2.2.2",
	"publisher": "AbhinabaGhosh",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Programming Languages"
	],
	"author": "Abhinaba Ghosh",
	"icon": "media/icon.png",
	"activationEvents": [
		"onCommand:execute.scenario",
		"onCommand:execute.feature"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "execute.scenario",
				"title": "Run Cucumber Scenario"
			},
			{
				"command": "execute.feature",
				"title": "Run Cucumber Feature"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "execute.scenario"
				},
				{
					"command": "execute.feature"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"dependencies": {
		"spawn-command": "0.0.2-1",
		"tree-kill": "^1.1.0"
	},
	"devDependencies": {
		"@types/vscode": "^1.67.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "^16.11.51",
		"eslint": "^8.22.0",
		"@typescript-eslint/parser": "^5.33.1",
		"@typescript-eslint/eslint-plugin": "^5.33.1",
		"glob": "^8.0.3",
		"mocha": "^10.0.0",
		"typescript": "^4.7.4",
		"vscode-test": "^1.6.1"
	}
}
